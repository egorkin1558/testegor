    print("")
    print(spisok2,end=" ")
    print('')
    a=int(input(""))
    b=int(input(""))

class v2():
    def __init__(self,a,b):
        if a>=2 and b==2:
            print("")
            c=int(input(""))
            spi = []
            while c != 1:
                oc = c % b
                c = c // b
                if oc==1:
                    spi.append(1)
                if oc==0:
                    spi.append(0)
                if c==1:
                    spi.append(1)

                    print("Ответ:",spi[::-1])



        if a >= 3 and b == 3:
            print("")
            c = int(input(""))
            spi = []
            while c != 1:
                oc = c % b
                c = c // b
                if oc == 2:
                    spi.append(2)
                if oc == 1:
                    spi.append(1)
                if oc == 0:
                    spi.append(0)
                if c == 1:
                    spi.append(1)
                if c == 2:
                    spi.append(2)

                    print("Ответ:", spi[::-1])

        if a>=8 and b==8:
            print("")
            c=int(input(""))
            spi = []
            while c != 1:
                oc = c % b
                c = c // b
                if oc <= 7:
                    spi.append(oc)
                if oc == 2:
                    spi.append(2)
                if oc == 1:
                    spi.append(1)
                if oc == 0:
                    spi.append(0)
                if c <= 7:
                    spi.append(c)

                    print("Ответ:", spi[::-1])

        if a>=10 and b==10:
            print("")
            c=int(input(""))
            spi = []
            while c != 1:
                oc = c % b
                c = c // b
                if oc <= 10:
                    spi.append(oc)
                if c <= 10:
                    spi.append(c)

                    print("Ответ:", spi[::-1])


        if a>=16 and b==16:
            print("")
            c=int(input(""))
            while True:
                pass


        else:
            pass
priem()
v2(a,b)
